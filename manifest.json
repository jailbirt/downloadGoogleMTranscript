{
  "manifest_version": 2,
  "name": "Google Meet Captions Capture",
  "version": "1.0",
  "description": "Capture captions from Google Meet",
  "permissions": ["activeTab", "tabs"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://meet.google.com/*"],
      "js": ["content.js"]
    }
  ]
}

